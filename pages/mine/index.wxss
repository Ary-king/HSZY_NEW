.model_wrapper_boxContImg {
	width: 30px;
	height: 30px;
}

.model_wrapper_boxTxt {
	color: rgba(0, 0, 0, 1);
	font-size: 36rpx;
	margin-top: 20rpx;
}

.model_wrapper_boxFont {
	color: rgba(0, 0, 0, 1);
